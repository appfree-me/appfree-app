{
    "name": "appfree/appfree",
    "type": "project",
    "description": "",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-pcntl": "*",
        "devristo/phpws": "dev-master",
        "educoder/pest": "1.0.0",
        "greenfieldtech-nirs/phpari": "dev-master"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "AppFree\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
