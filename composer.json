{
    "name": "appfree-me/appfree-app",
    "description": "AppFree main Laravel application",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "framework"
    ],
    "require": {
        "php": "^8.3",
        "ext-pcntl": "*",
        "appfree-me/php-asterisk-swagger-api": "0.1",
        "ergebnis/composer-normalize": "^2.45",
        "evenement/evenement": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "12.1.1.0",
        "laravel/tinker": "^2.9",
        "ratchet/pawl": "^0.4.1",
        "react/async": "^4.3",
        "yohang/finite": "^1.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.70",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpdoc-parser": "^1.18",
        "phpstan/phpstan": "^2.1"
    },
    "repositories": {
        "php-asterisk-swagger-api": {
            "type": "package",
            "package": {
                "name": "appfree-me/php-asterisk-swagger-api",
                "version": "0.1",
                "source": {
                    "type": "git",
                    "url": "https://github.com/appfree-me/php-asterisk-swagger-api.git",
                    "reference": "origin/master"
                }
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AppFree\\": "app/",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Swagger\\Client\\": "vendor/appfree-me/php-asterisk-swagger-api/lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.dist', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "deploy": [
            "@php vendor/bin/dep -vv deploy"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "lint": [
            "@php ./vendor/bin/php-cs-fixer fix --verbose --dry-run --diff --allow-risky=yes",
            "@php ./vendor/bin/phpmd app text ruleset.xml",
            "@php ./vendor/bin/phpstan analyse app"
        ],
        "lint:fix": [
            "@php ./vendor/bin/php-cs-fixer fix --diff --allow-risky=yes",
            "@composer normalize"
        ],
        "test": [
            "@php ./vendor/bin/pest"
        ]
    }
}
